{
  "name": "generator-ngx-library",
  "version": "5.4.0",
  "description": "Yeoman generator to bootstrap Angular library creation and publication, with integrated demo app, continuous integration/deployment system, code coverage status and much more!",
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "angularjs",
    "angular",
    "plugin",
    "component",
    "directive",
    "library",
    "project",
    "quickstart",
    "initializer",
    "starter"
  ],
  "homepage": "http://tinesoft.github.io/",
  "bugs": "https://github.com/tinesoft/generator-ngx-library/issues",
  "author": {
    "name": "Tine Kondo",
    "email": "kondotine@gmail.com",
    "url": "https://github.com/tinesoft"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/tinesoft/generator-ngx-library.git"
  },
  "scripts": {
    "test": "jest",
    "check-appveyor": "appveyor-status --project=tinesoft/generator-ngx-library --branch=master --wait || echo 'Not yet passing'",
    "check-travis": " travis-status --repo=tinesoft/generator-ngx-library --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "mkdirp": "0.5.1",
    "yeoman-generator": "2.0.1",
    "yosay": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "22.0.1",
    "@types/yeoman-generator": "2.0.0",
    "appveyor-status": "2.0.1",
    "commitplease": "3.1.0",
    "conventional-github-releaser": "2.0.0",
    "eslint": "4.8.0",
    "eslint-config-xo-space": "0.17.0",
    "glob": "^7.1.2",
    "greenkeeper-lockfile": "1.12.0",
    "gulp": "3.9.1",
    "gulp-bump": "2.8.0",
    "gulp-conventional-changelog": "1.1.6",
    "gulp-coveralls": "0.1.4",
    "gulp-eslint": "3.0.1",
    "gulp-git": "2.4.2",
    "gulp-nsp": "2.4.2",
    "gulp-plumber": "1.1.0",
    "gulp-shell": "0.6.3",
    "gulp-util": "3.0.8",
    "jest": "22.0.5",
    "lodash": "4.17.4",
    "markdown-toc": "1.2.0",
    "run-sequence": "2.2.0",
    "semver": "5.3.0",
    "through2": "2.0.3",
    "travis-status": "2.0.0",
    "yargs": "10.0.3",
    "yeoman-assert": "3.1.0",
    "yeoman-test": "1.7.0"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  },
  "jest":{
    "testMatch": [ "**/__tests__/**/*.js"],
    "testPathIgnorePatterns": [
      "templates"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/templates/**"
    ],
    "coverageDirectory": "coverage",
    "mapCoverage": true,
    "coverageReporters": [ "lcov", "text" ],
    "coveragePathIgnorePatterns": [ "/node_modules/", "/integrations/", "/recipes/", "/assets/", "/dist/"]
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
